{
  "extends": [
    "next",
    "next/core-web-vitals",
    "plugin:cypress/recommended",
    "plugin:prettier/recommended",
    "plugin:react-hooks/recommended"
  ],
  "globals": {
    "React": "readonly"
  },
  "plugins": ["cypress", "prettier", "react-hooks"],
  "rules": {
    "no-console": ["error", { "allow": ["info", "warn", "error"] }],
    "no-alert": "error"
  }
}
